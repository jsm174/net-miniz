<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/01/nuspec.xsd">
  <metadata>
    <!-- package -->
    <id>NetMiniZ.Native</id>
    <title>NetMiniZ - Native binaries</title>
    <version>1.2.0</version>
    <description>This package complements the NetMiniZ package and contains native binaries of libminiz</description>
    <summary>Native binaries of libminiz</summary>
    <projectUrl>https://github.com/jsm174/net-miniz</projectUrl>
    <repository type="git" url="https://github.com/jsm174/net-miniz" />
    <tags>libminiz binaries</tags>

    <!-- legal -->
    <license type="expression">MIT</license>
    <authors>Jason Millard</authors>
    <owners>Jason Millard</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <copyright>Jason Millard</copyright>

    <dependencies>
      <group targetFramework="netstandard2.0">
        <dependency id="NetMiniZ.Native.linux-x64" version="1.2.0" include="native" />
        <dependency id="NetMiniZ.Native.osx-x64" version="1.2.0" include="native" />
        <dependency id="NetMiniZ.Native.win-x64" version="1.2.0" include="native, build" />
        <dependency id="NetMiniZ.Native.win-x86" version="1.2.0" include="native, build" />
      </group>
    </dependencies>
  </metadata>

  <files>
    <!-- A dummy reference which prevents NuGet from adding any compilation references when this package is imported -->
    <file src="_._" target="lib/netstandard2.0" />
  </files>
</package>
